programa {
  funcao inicio() {
    
  }
}
Algoritmo OrdenarVetorDecrescente
Var
   vetor: Vetor[1..10] de Inteiro
   i, j, temp: Inteiro

Inicio

   Para i de 1 ate 10 faca
      Escreva("Digite o valor ", i, ": ")
      Leia(vetor[i])
   FimPara

   Para i de 1 ate 10 faca
      Para j de 1 ate 10 - i faca
         Se vetor[j] < vetor[j + 1] Entao
           
            temp <- vetor[j]
            vetor[j] <- vetor[j + 1]
            vetor[j + 1] <- temp
         FimSe
      FimPara
   FimPara

  
   Escreva("Vetor ordenado em ordem decrescente: ")
   Para i de 1 ate 10 faca
      Escreva(vetor[i], " ")
   FimPara
FimAlgoritmo
